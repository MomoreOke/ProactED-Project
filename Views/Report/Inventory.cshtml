@model IEnumerable<FEENALOoFINALE.Models.InventoryItem>
@{
    ViewData["Title"] = "Inventory Report";
}
<h1>Inventory Report</h1>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Min Stock Level</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Category</td>
                <td>@item.MinStockLevel</td>
            </tr>
        }
    </tbody>
</table>